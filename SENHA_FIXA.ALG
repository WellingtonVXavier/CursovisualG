Algoritmo "Senha_Fixa"

Var

   senha : caractere

Inicio


   escreva("Digite sua senha: ")
   leia(senha)

   enquanto senha <> "2002" faca
      escreval("SENHA INVALIDA!")
      escreval()
      escreva("Tente Novamente")
      escreval()
      escreva("Digite sua senha: ")
      leia(senha)
   fimenquanto

   escreval("Acesso Permitido!")


Fimalgoritmo